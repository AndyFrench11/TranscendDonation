<AnchorPane fx:id="transplantPane" minHeight="0.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" stylesheets="@../css/darkblue.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.Controllers.TransplantWaitingListController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="800.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" onAction="#close"/>
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="transplantTable" layoutX="56.0" layoutY="100.0" prefHeight="458.0" prefWidth="682.0">
        <columns>
          <TableColumn fx:id="organColumn" prefWidth="115.0" text="Organ" />
          <TableColumn fx:id="firstNameColumn" prefWidth="170.0" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="170.0" text="Last Name" />
            <TableColumn fx:id="dateColumn" prefWidth="95.0" text="Date" />
            <TableColumn fx:id="regionColumn" prefWidth="135.0" text="Region" />
        </columns>
      </TableView>
      <Button layoutX="614.0" layoutY="48.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="121.0" text="Return" onAction="#returnView" />
